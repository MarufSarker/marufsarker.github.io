  "main": "gulpfile.js",
  "scripts": {
    "task-gulp-build": "node ./node_modules/gulp/bin/gulp.js --gulpfile ./gulpfile.js gulp-build",
    "gulp-build-dev": "node ./node_modules/cross-env/dist/bin/cross-env.js NODE_ENV=development npm run task-gulp-build",
    "gulp-build": "node ./node_modules/cross-env/dist/bin/cross-env.js NODE_ENV=production npm run task-gulp-build",
    "run-gulp": "node ./node_modules/gulp/bin/gulp.js --gulpfile ./gulpfile.js",
    "start-dev": "node ./node_modules/cross-env/dist/bin/cross-env.js NODE_ENV=development npm run run-gulp",
    "start": "node ./node_modules/cross-env/dist/bin/cross-env.js NODE_ENV=production npm run run-gulp",
    "clean": "node ./node_modules/gulp/bin/gulp.js --gulpfile ./gulpfile.js clean"
  },
  "dependencies": {
    "lunr": "^1.0.0"
  },
